<h1>Edit Product</h1>

<%= form_for @product do |f| %>
	<div>
	<%= f.label :name %>
	<%= f.text_field :name %>

	<%= f.label :description %>
	<%= f.text_field :description %>

	<%= f.label :price %>
	<%= f.text_field :price %>

	<%= f.label :photo %>
	<%= f.text_field :photo %>


	</div>

	<div>
		<%= f.submit %>
	</div>

<% end %>
